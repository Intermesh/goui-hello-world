{
  "devDependencies": {
    "concurrently": "^7.6.0",
    "esbuild": "^0.16.17",
    "sass": "^1.58.3",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "concurrently --kill-others \"npm run start:serve\"  \"npm run start:sass\"",
    "start:serve": "npx esbuild ./script/Index.ts --bundle --serve=localhost:8081 --servedir=. --external:./dist/* --external:./script/*  --sourcemap --format=esm --target=esnext --outdir=dist",
    "start:sass": "npx sass --watch ./style:./dist"
  },
  "dependencies": {
    "@intermesh/goui": "^1.0.1"
  }
}
